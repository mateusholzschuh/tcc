<%- include('../partials/header') %>

<% ues.forEach((e, idx) => { %>
<div class="grid-stack-item ui-draggable ui-resizable ui-resizable-autohide ui-draggable-disabled ui-resizable-disabled"
    data-gs-no-resize="1" data-gs-no-move="1" data-gs-x="<%- 1+idx*4 %>" data-gs-y="0" data-gs-width="4" data-gs-height="6"
    data-gs-min-width="4" data-gs-min-height="6" data-antares-grid-index="<%- idx %>" style="z-index: 0;">
    <div class="grid-stack-item-content">

        <div class="card card--clean card--zd">
            <div class="card__header">
                <div class="card__header--left"><%- e.event.name %> <small>(<%- e.role %>)</small></div>
            </div>
            <div class="card__content">
                <div class="zd zd--sm">
                    <div class="zd__header">
                        <i class="zmdi zmdi-globe"></i>
                    </div>
                    <div class="zd__content">
                        <div class="zd__title">Acessar evento "<strong>
                                <%- e.event.name %></strong>"</div>

                    </div>
                    <div class="zd__footer">
                        <a href="/events/<%- e.event._id %>/" class="btn btn--primary btn--zd mdl-js-button mdl-js-ripple-effect"
                            data-upgraded=",MaterialButton,MaterialRipple">Acessar<span class="mdl-button__ripple-container"><span
                                    class="mdl-ripple"></span></span></a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<% }) %>

<% if(!locals.ues || ues.length == 0) { %>
<h2>Nenhum compromisso para vocÃª!</h2>
<% } %>

<%- include('../partials/footer') %>